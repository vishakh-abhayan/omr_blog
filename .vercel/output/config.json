{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/category\\/([^/]+?)\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/post(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tags\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tags(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}
